<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<sbe:messageSchema package="broke.fix.dto" id="1" xmlns:sbe="http://fixprotocol.io/2016/sbe">
	<types>
		<composite name="messageHeader" description="Message identifiers and length of message root">
			<type name="blockLength" primitiveType="uint16"/>
			<type name="templateId" primitiveType="uint16"/>
			<type name="schemaId" primitiveType="uint16"/>
			<type name="version" primitiveType="uint16"/>
		</composite>
		<enum name="ExecInst" encodingType="char">
			<validValue name="NotHeld">1</validValue>
			<validValue name="Suspend">S</validValue>
		</enum>
		<type name="Symbol" primitiveType="char" length="4" characterEncoding="ASCII"/>
		<type name="ClOrdID" primitiveType="char" length="16" characterEncoding="ASCII"/>
	</types>
	<sbe:message name="NewOrderSingle" id="1">
		<field name="symbol" id="1" type="Symbol"/>
		<field name="execInst" id="2" type="ExecInst"/>
		<field name="clOrdID" id="3" type="ClOrdID"/>
		<field name="orderQty" id="4" type="int64"/>
		<field name="price" id="5" type="double"/>
	</sbe:message>
</sbe:messageSchema>
